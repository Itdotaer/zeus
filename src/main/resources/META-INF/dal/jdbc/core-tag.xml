                <param name="name"/>
                <statement>
                    <![CDATA[
                    SELECT <FIELDS/>
                    FROM <TABLE/>
                    WHERE <FIELD name='name'/> = ${name}
                    ]]>
                </statement>
            </query>
            <query name="insert" type="INSERT">
                <statement>
                    <![CDATA[
                    INSERT INTO <TABLE/>(<FIELDS/>)
                    VALUES(<VALUES/>)

        </query-defs>
    </entity>
    <entity name="slb-domain" table="slb_domain" alias="sd" do-class="SlbDomainDo">
        <query-defs>
            <query name="insert" type="INSERT">
                <statement>
                    <![CDATA[
                    INSERT INTO <TABLE/>(<FIELDS/>)
                    VALUES(<VALUES/>)
                    ON DUPLICATE KEY UPDATE
                        slb_virtual_server_id = values(slb_virtual_server_id),
                        name = values(name),

<?xml version="1.0" encoding="UTF-8"?>
<entities do-package="com.ctrip.zeus.dal.core" gen="true" do-class-suffix="Do">
    <entity name="app" table="app" alias="a" do-class="AppDo">
        <var name="ids" value-type="long[]"/>
        <var name="names" value-type="String[]"/>
        <var name="max-count" value-type="int"/>
        <query-defs>
            <query name="insert" type="INSERT">
                <statement>
                    <![CDATA[
                    INSERT INTO <TABLE/>(<FIELDS/>)
                    VALUES(<VALUES/>)
                    ]]>
                </statement>
            </query>
            <query name="update-by-PK" type="UPDATE">
                <param name="id"/>
                <statement>
                    <![CDATA[
                    UPDATE <TABLE/>
                    SET name = values(name),
                        app_id = values(app_id),
                        version = version + 1,
                        <FIELD name='last-modified'/> = NOW()
                    WHERE <FIELD name='id'/> = ${id}
                    ]]>
                </statement>
            </query>
            <query name="find-all" type="SELECT" multiple="true">
                <statement>
                    <![CDATA[
                    SELECT <FIELDS/>
                    FROM <TABLE/>
                    ]]>
                </statement>
            </query>
            <query name="find-limit" type="SELECT" multiple="true">
                <param name="id"/>
                <param name="max-count"/>
                <statement>
                    <![CDATA[
                    SELECT <FIELDS/>
                    FROM <TABLE/>
                        WHERE <FIELD name='id'/> > ${id}
                        LIMIT ${max-count}
                    ]]>
                </statement>
            </query>
            <query name="find-by-name" type="SELECT">
                <param name="name"/>
                <statement>
                    <![CDATA[
                    SELECT <FIELDS/>
                    FROM <TABLE/>
                        WHERE <FIELD name='name'/> = ${name}
                    ]]>
                </statement>
            </query>
            <query name="find-by-appId" type="SELECT">
                <param name="app-id"/>
                <statement>
                    <![CDATA[
                    SELECT <FIELDS/>
                    FROM <TABLE/>
                        WHERE <FIELD name='app-id'/> = ${app-id}
                    ]]>
                </statement>
            </query>
            <query name="find-all-by-ids" type="SELECT" multiple="true">
                <param name="ids"/>
                <statement>
                    <![CDATA[
                    SELECT <FIELDS/>
                    FROM <TABLE/>
                        WHERE <FIELD name='id'/> in <IN>${ids}</IN>
                    ]]>
                </statement>
            </query>
            <query name="find-all-by-names" type="SELECT" multiple="true">
                <param name="names"/>
                <statement>
                    <![CDATA[
                    SELECT <FIELDS/>
                    FROM <TABLE/>
                        WHERE <FIELD name='name'/> in <IN>${names}</IN>
                    ]]>
                </statement>
            </query>
        </query-defs>
    </entity>

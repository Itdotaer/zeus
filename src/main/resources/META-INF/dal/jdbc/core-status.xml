<?xml version="1.0" encoding="UTF-8"?>
<entities do-package="com.ctrip.zeus.dal.core" gen="true" do-class-suffix="Do">
    <entity name="status-app-server" table="status_app_server" alias="sas" do-class="StatusAppServerDo">
        <query-defs>
            <query name="insert" type="INSERT">
                <statement>
                    <![CDATA[
                    INSERT INTO <TABLE/>(<FIELDS/>)
                    VALUES(<VALUES/>)
                    ON DUPLICATE KEY UPDATE
                        up = values(up),
                        <FIELD name="last-modified"/> = NOW()
                    ]]>
                </statement>
            </query>
            <query name="find-all" type="SELECT" multiple="true">
                <statement>
                    <![CDATA[
                    SELECT <FIELDS/>
                    FROM <TABLE/>
                    ]]>
                </statement>
            </query>
            <query name="find-all-by-app" type="SELECT" multiple="true">
                <param name="app-name"/>
                <statement>
                    <![CDATA[
                    SELECT <FIELDS/>
                    FROM <TABLE/>
                        WHERE <FIELD name='app-name'/> = ${app-name}
                    ]]>
                </statement>
            </query>
            <query name="find-all-by-ip" type="SELECT" multiple="true">
                <param name="ip"/>
                <statement>
                    <![CDATA[
                    SELECT <FIELDS/>
                    FROM <TABLE/>
                        WHERE <FIELD name='ip'/> = ${ip}
                    ]]>
                </statement>
            </query>
            <query name="find-all-by-slb-name-and-is-up" type="SELECT" multiple="true">
                <param name="slb-name"/>
                <param name="up"/>
                <statement>
                    <![CDATA[
                    SELECT <FIELDS/>
                    FROM <TABLE/>
                        WHERE <FIELD name='slb-name'/> = ${slb-name}
                        AND <FIELD name='up'/> = ${up}
                    ]]>
                </statement>
            </query>
        </query-defs>
    </entity>
